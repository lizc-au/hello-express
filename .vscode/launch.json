{
  "version": "0.2.0",
  "configurations": [
    {
      "cwd": "${workspaceFolder}",
      "skipFiles": ["<node_internals>/**"],
      "request": "launch",
      "console": "integratedTerminal",
      "name": "Run: npm run dev",
      "type": "node",
      "runtimeArgs": ["run", "dev"],
      "runtimeExecutable": "npm",
      "autoAttachChildProcesses": true
    }
  ]
}
